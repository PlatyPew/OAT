{
  "manifest_version": 3,
  "name": "OAT",
  "description": "OAT Handler",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html",
    "default_title": "OAT"
  },
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": [
        "backgroundNode.js"
      ]
    }
  ],
  "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
  }
}
